
@{
    ViewBag.Title = "MatchEntry";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles {
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>ပွဲစဉ်များဖြည့်သွင်းရန်</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">Match Entry</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
<!-- Main content -->
<section class="content">
    <div class="form-row">
        <div class="form-group col-xl-2 col-md-4">
            <label for="StaffID">ပြသသည့်ရက်</label>
            <div class="input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text text-custom2" id="basic-addon1"><i class="fa fa-calendar-alt"></i></span>
                </div>
                <input type="text" class="form-control float-right" id="showdate">
            </div>
        </div>
        <div class="form-group col-xl-2 col-md-4">
            <label for="StaffID">ဇာတ်ကား</label>
            <div class="input-group">
                <div class="input-group-prepend">
                    <span class="input-group-text text-custom2" id="basic-addon1"><i class="fas fa-film"></i></span>
                </div>

                @Html.DropDownListFor(cim => cim.MovieID, new SelectList(Enumerable.Empty<SelectListItem>(), "", ""), "---Select---",new
                {
                    @class = "form-control select2bs4 input-sm",
                    @id = "ddlMovie",
                    @tabindex = "15",
                })
            </div>
        </div>
    </div>
    <div id="CinemaList">
    </div>
</section>


@section Scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            
        });

    </script>
}


